}

body #header a.on {

background-color: #ff1e00;
color: white;

}


.mission {

position: absolute;
top: 200px;
width: 500px;
padding: 10px;

background-color: white;
border: 1px dotted #ff1e00;
color: black;
font-size: 28px;


}

.upcomingEventsWrapper {

position: absolute;
top: 500px;
width: 520px;


}

.upcomingEventsWrapper img {

border-bottom: 0px solid transparent;

}

.upcomingEvents {

position: relative;
padding: 10px;
background: white;
border: 1px dotted #ff1e00;
color: black;

}

.eventImage {

position: relative;
float: left;
width: 100px;

}

.eventInfo {

position: relative;
left: 20px;
float: left;
line-height: 1.35;

}


a.eventTitle  {

color: #ff1e00;
padding: 0px;
font-family: 'Merriweather';
font-size: 14px;
font-weight: normal;
border-bottom: 0px;
margin-left: 0px;

}

a.eventTitle:hover {

text-decoration: underline;
cursor: pointer;
}

a.event {

cursor: pointer;
border-bottom: 0px;

}

.divider {

color: #d0d0d0;
font-size: 9px;
line-height: 1.8;
}

hr.divider{
	height: 1px;
	color:#d0d0d0; 
	border: solid #d0d0d0;
	border-style: solid none none none;
}

#contentHome {

position: relative;
left: 540px;
top: 33px;
width: 350px;


}

#homeLeft {

background-color: transparent;

}

#homePost {

background-color: red;

}



